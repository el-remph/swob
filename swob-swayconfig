bindsym --locked XF86MonBrightnessDown	exec swobctl.sh brightness 5%-
bindsym --locked XF86MonBrightnessUp	exec swobctl.sh brightness 5%+
bindsym --locked XF86AudioLowerVolume	exec swobctl.sh volume 5%-
bindsym --locked XF86AudioRaiseVolume	exec swobctl.sh volume 5%+
bindsym --locked XF86AudioMute		exec swobctl.sh volume toggle
